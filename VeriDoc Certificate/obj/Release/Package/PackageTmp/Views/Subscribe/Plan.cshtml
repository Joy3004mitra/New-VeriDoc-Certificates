    <script type="text/javascript" language="javascript">

        function validationFirstStep() {
            if (document.getElementById("txt_FistName").value == "") {
                document.getElementById("sp_FiratName").innerHTML = "Enter First Name";
                document.getElementById("txt_FistName").focus();
                return false;
            }
            else if (document.getElementById("txt_LastName").value == "") {
                document.getElementById("sp_LastName").innerHTML = "Enter Last Name";
                document.getElementById("txt_LastName").focus();
                return false;
            }
            else if (document.getElementById("txt_Eamil").value == "") {
                document.getElementById("sp_Email").innerHTML = "Enter Email Id";
                document.getElementById("txt_Eamil").focus();
                return false;
            }
            var emailPat = /^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/;
            var emailid = document.getElementById("txt_Eamil").value;
            var matchArray = emailid.match(emailPat);
            if (matchArray == null) {
                document.getElementById("sp_Email").innerHTML = "Enter valid email address";
                document.getElementById("txt_Eamil").focus();
                return false;
            }
            else if (document.getElementById("txt_Phone").value == "") {

                document.getElementById("sp_Phone").innerHTML = "Enter Phone Number";
                document.getElementById("txt_Phone").focus();
                return false;
            }
            var phonePat = '^[0-9]{0,10}$';
            var phone = document.getElementById("txt_Phone").value;
            var matchPhone = phone.match(phonePat);
            if (matchPhone == null) {
                document.getElementById("sp_Phone").innerHTML = "Enter valid Phone Number";
                document.getElementById("txt_Phone").focus();
                return false;
            }
            else {
                $("#company_details").show();
                $("#contact_details").hide();
            }
        }

        function validation2() {
            if (document.getElementById("txt_Company_Name").value == "") {
                document.getElementById("sp_Company_Name").innerHTML = "Enter Company Name";
                document.getElementById("txt_Company_Name").focus();
                return false;
            }
            else if (document.getElementById("txt_Company_Address").value == "") {
                document.getElementById("sp_Company_Address").innerHTML = "Enter Company Address";
                document.getElementById("txt_Company_Address").focus();
                return false;
            }
            else if (document.getElementById("txt_City").value == "") {

                document.getElementById("sp_City").innerHTML = "Company City Filed can not be blank";
                document.getElementById("txt_City").focus();
                return false;
            }
            else {
                $("#pricing_details").show();
                $("#company_details").hide();
            }
        }

        function validation(e) {
            if (document.getElementById("txt_FistName").value == "") {
                document.getElementById("sp_FiratName").innerHTML = "Enter First Name";
                document.getElementById("txt_FistName").focus();
                return false;
            }

            else if (document.getElementById("txt_LastName").value == "") {
                document.getElementById("sp_LastName").innerHTML = "Enter Last Name";
                document.getElementById("txt_LastName").focus();
                return false;
            }

            else if (document.getElementById("txt_Eamil").value == "") {
                document.getElementById("sp_Email").innerHTML = "Enter Email Id";
                document.getElementById("txt_Eamil").focus();
                return false;
            }
            var emailPat = /^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/;
            var emailid = document.getElementById("txt_Eamil").value;
            var matchArray = emailid.match(emailPat);
            if (matchArray == null) {
                document.getElementById("sp_Email").innerHTML = "Enter valid email address";
                document.getElementById("txt_Eamil").focus();
                return false;
            }
            else if (document.getElementById("txt_Phone").value == "") {

                document.getElementById("sp_Phone").innerHTML = "Enter Phone Number";
                document.getElementById("txt_Phone").focus();
                return false;
            }
            var phonePat = '^[0-9]{0,10}$';
            var phone = document.getElementById("txt_Phone").value;
            var matchPhone = phone.match(phonePat);
            if (matchPhone == null) {
                document.getElementById("sp_Phone").innerHTML = "Enter valid Phone Number";
                document.getElementById("txt_Phone").focus();
                return false;
            }

            if (document.getElementById("txt_Company_Name").value == "") {

                document.getElementById("sp_Company_Name").innerHTML = "Enter Company Name";
                document.getElementById("txt_Company_Name").focus();
                return false;
            }
            else if (document.getElementById("txt_Company_Address").value == "") {

                document.getElementById("sp_Company_Address").innerHTML = "Enter Company Address";
                document.getElementById("txt_Company_Address").focus();
                return false;
            }
            else if (document.getElementById("txt_City").value == "") {

                document.getElementById("sp_City").innerHTML = "Enter City Name";
                document.getElementById("txt_City").focus();
                return false;
            }
            else if (document.getElementById("show").checked) {
                if (document.getElementById("txt_Alter_Company_Name").value == "") {
                    document.getElementById("billing_companyName").innerHTML = "Enter Company Name";
                    document.getElementById("txt_Alter_Company_Name").value = "";
                    document.getElementById("txt_Alter_Company_Name").focus();
                    return false;
                }

                if (document.getElementById("txt_Alter_Company_Address").value == "") {
                    document.getElementById("billing_companyAddress").innerHTML = "Enter Company Address";
                    document.getElementById("txt_Alter_Company_Address").value = "";
                    document.getElementById("txt_Alter_Company_Address").focus();
                    return false;
                }

                if (document.getElementById("txt_Alter_City").value == "") {
                    document.getElementById("billing_companyCity").innerHTML = "Enter Company City";
                    document.getElementById("txt_Alter_City").value = "";
                    document.getElementById("txt_Alter_City").focus();
                    return false;
                }

                if (document.getElementById("txt_Alter_State").value == "") {
                    document.getElementById("billing_companyState").innerHTML = "Enter Company State";
                    document.getElementById("txt_Alter_State").value = "";
                    document.getElementById("txt_Alter_State").focus();
                    return false;
                }

                if (document.getElementById("txt_Alter_ZIP").value == "") {
                    document.getElementById("billing_companyZip").innerHTML = "Enter Company ZIP Code";
                    document.getElementById("txt_Alter_ZIP").value = "";
                    document.getElementById("txt_Alter_ZIP").focus();
                    return false;
                }



            }
            validateBillingData(e);
        }

        function error() {
            document.getElementById("sp_FiratName").innerHTML = "";
            document.getElementById("sp_LastName").innerHTML = "";
            document.getElementById("sp_Email").innerHTML = "";
            document.getElementById("sp_Phone").innerHTML = "";
            document.getElementById("sp_Company_Name").innerHTML = "";

            document.getElementById("sp_Company_Address").innerHTML = "";
            document.getElementById("sp_City").innerHTML = "";
        }

        function validateBillingData(e) {
            var sameAddre = $('input[name=SameBillingDetails]:checked').val();
            if (sameAddre == "yes") {
                $('#txt_Alter_Company_Name').val($('#txt_Company_Name').val());
                $('#txt_Alter_Company_Address').val($('#txt_Company_Address').val());
                $('#txt_Alter_City').val($('#txt_City').val());
                $('#txt_Alter_State').val($('#txt_State').val());
                $('#txt_Alter_ZIP').val($('#txt_ZIP').val());
                $('#BillingCompanyCountry').val($('#CompanyCountry').val());
            }
        }

        $(document).ready(function () {
            $("#hide").click(function () {
                $(".billing_address").hide();
            });

            $("#show").click(function () {
                $(".billing_address").show();
            });

            $(".back").click(function () {
                $("#company_details").hide();
                $("#contact_details").show();
            });

            $(".last_back").click(function () {
                $("#pricing_details").hide();
                $("#company_details").show();
            });

            $("#submitBtn").click(function (e) {
                validation(e);
            });

            $("#submitMobileBtn").click(function (e) {
                validateBillingData(e);
                $("#submitBtn").click();
            });

            $(".fist_step").click(function () {
                validationFirstStep();

            });

            $(".sce_step").click(function () {
                validation2();
            });

            $(".form-control").on("click", function () {
                error();
            });

        });
    </script>
