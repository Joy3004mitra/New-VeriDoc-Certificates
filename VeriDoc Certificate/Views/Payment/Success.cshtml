<body class="home-1">
    <div class="main-area">
        <div class="pay-link-header">
            VERIDOC GLOBAL PTY LTD
        </div>

        <div class="pay-page">
            <div class="right-col">
                <section class="pay-page-content col-card">
                    <div class="card-header">
                        <div class="pay-page-info">
                            <div class="animatedcheck">
                                <svg version="1.1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 130.2 130.2">
                                    <circle class="path circle" fill="none" stroke="#73AF55" stroke-width="6" stroke-miterlimit="10" cx="65.1" cy="65.1" r="62.1" />
                                    <polyline class="path check" fill="none" stroke="#73AF55" stroke-width="6" stroke-linecap="round" stroke-miterlimit="10" points="100.2,40.2 51.5,88.8 29.8,67.5 " />
                                </svg>
                            </div>
                            <h3 class="soc-product-title">Payment Successful !</h3>
                            <h3 class="soc-product-price">
                                <span class="price">@ViewBag.PlanName package : AU$@ViewBag.PlanPriceAU </span><span class="cadence">/ month</span>
                            </h3>
                        </div>
                    </div>

                    <div class="form-section mt-3 successform-section">
                        <hr class="form-footer-divider" style="margin-top: 24px; margin-bottom: 24px;">

                        <p class="pay-link-description description successparagraph">Thank you for your payment. We have begun setting up your administrator account. We will send you an email confirmation as soon as your account is ready to create new certificates.</p>

                        <div class="form-button pay-button">
                            <a href="~/"><i class="material-icons">home</i> &nbsp; Back to Home</a>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
    <!-- End main area -->

    <footer class="squarefooter">
